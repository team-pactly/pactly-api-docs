    "license": "Apache-2.0",
    "devDependencies": {
      "@redocly/cli": "^1.6.0",
      "http-server": "^14.1.1"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/pactly/pactly-api-docs.git"
    }
  }